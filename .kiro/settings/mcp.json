{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": [
        "fetch"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "Framelink Figma MCP": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=YOUR-KEY",
        "--stdio"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "mcp-deepwiki": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-deepwiki@latest"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ],
      "disabled": false,
      "autoApprove": [
        "read_file",
        "list_directory",
        "create_directory"
      ]
    },
    "structured-thinking": {
      "command": "npx",
      "args": [
        "structured-thinking"
      ],
      "disabled": false,
      "autoApprove": [
        "capture_thought",
        "capture_thought"
      ]
    },
    "firecrawl-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "fc-3adcc026d6ac4ee0a1647899f1573021"
      },
      "disabled": false,
      "autoApprove": [
        "firecrawl_search",
        "firecrawl_scrape"
      ]
    }
  }
}